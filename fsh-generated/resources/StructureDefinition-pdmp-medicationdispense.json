  "differential": {
    "element": [
      {
        "id": "MedicationDispense",
        "path": "MedicationDispense",
        "definition": "Dispense of a medication that was prescribed for a patient.",
        "mustSupport": false
      },
      {
        "id": "MedicationDispense.extension",
        "path": "MedicationDispense.extension",
        "slicing": {
          "discriminator": [
            {
              "type": "value",
              "path": "url"
            }
          ],
          "ordered": false,
          "rules": "open"
        }
      },
      {
        "id": "MedicationDispense.extension:rx-refill-number",
        "path": "MedicationDispense.extension",
        "sliceName": "rx-refill-number",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/us/pdmp/StructureDefinition/pdmp-extension-rx-refill-number"
            ]
          }
        ],
        "mustSupport": true,
        "mapping": [
          {
            "identity": "pdmp-medication-asap-ws",
            "map": "DispensingEvent/FillNumber"
          }
        ]
      },
      {
        "id": "MedicationDispense.extension:rx-transmission-method",
        "path": "MedicationDispense.extension",
        "sliceName": "rx-transmission-method",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "Extension",
            "profile": [
              "http://hl7.org/fhir/us/pdmp/StructureDefinition/pdmp-extension-rx-transmission-method"
            ]
          }
        ],
        "mustSupport": true
      },
      {
        "id": "MedicationDispense.identifier",
        "path": "MedicationDispense.identifier",
        "mapping": [
          {
            "identity": "pdmp-medication-asap-ws",
            "map": "DispensingEvent/PrescriptionNumber"
          }
        ]
      },
      {
        "id": "MedicationDispense.status",
        "path": "MedicationDispense.status",
        "binding": {
          "strength": "required",
          "valueSet": "http://hl7.org/fhir/ValueSet/medicationdispense-status",
          "description": "A set of codes indicating the current status of a MedicationDispense."
        }
      },
      {
        "id": "MedicationDispense.medication[x]",
        "path": "MedicationDispense.medication[x]",
        "slicing": {
          "discriminator": [
            {
              "type": "type",
              "path": "$this"
            }
          ],
          "ordered": false,
          "rules": "open"
        },
        "binding": {
          "strength": "extensible",
          "valueSet": "http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113762.1.4.1010.4",
          "description": "Prescribable medications"
        }
      },
      {
        "id": "MedicationDispense.medication[x]:medicationCodeableConcept",
        "path": "MedicationDispense.medication[x]",
        "sliceName": "medicationCodeableConcept",
        "min": 0,
        "max": "1",
        "type": [
          {
            "code": "CodeableConcept"
          }
        ]
      },
      {
        "id": "MedicationDispense.medication[x]:medicationCodeableConcept.text",
        "path": "MedicationDispense.medication[x].text",
        "mapping": [
          {
            "identity": "pdmp-medication-asap-ws",
            "map": "DispensingEvent/DrugName"
          }
        ]
      },
